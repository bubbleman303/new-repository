<%@page contentType="text/html" pageEncoding="UTF-8"%><!DOCTYPE html><html>    <head>               <%!   String check(int num){  if(num%2==0){           return "は偶数です";}else{      return"は奇数です";  }}      %>              <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">        <title>JSP Page</title>    </head>    <body>                    
<%    int x=2;out.print(x+check(x));%>    </body></html>
